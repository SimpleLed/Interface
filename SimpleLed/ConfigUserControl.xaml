<UserControl x:Class="SimpleLed.ConfigUserControl"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:SimpleLed"
             mc:Ignorable="d" 
             d:DesignHeight="450" d:DesignWidth="800">
    <UserControl.Resources>
        <Color x:Key="AutoForegroundColor" A="255" R="0" G="0" B="0" />
        <SolidColorBrush x:Key="AutoForegroundBrush" Color="{DynamicResource AutoForegroundColor}"/>

        <Color x:Key="AutoBackgroundColor" A="255" R="0" G="0" B="0" />
        <SolidColorBrush x:Key="AutoBackgroundBrush" Color="{DynamicResource AutoBackgroundColor}"/>
    </UserControl.Resources>
    <Grid>
            
    </Grid>
</UserControl>
